{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "checkJs": false,
    "skipLibCheck": true,
    "types": []
  },
  "typeAcquisition": {
    "enable": false
  },
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ],
  "include": [
    "src"
  ]
}